{
  "app": {
    "bundleName": "com.hmosdemos.golfgpsfinderlite",
    "vendor": "hmosdemos",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "deviceType": [
      "liteWearable"
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.LOCATION",
        "reason": "$string:reason_description"
      }
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "name": ".MainAbility",
        "srcLanguage": "js",
        "srcPath": "MainAbility",
        "icon": "$media:icon",
        "description": "$string:MainAbility_desc",
        "label": "$string:MainAbility_label",
        "type": "page"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index",
          "pages/clubs/clubs",
          "pages/clubDetail/clubDetail",
          "pages/game/game"
        ],
        "name": ".MainAbility"
      }
    ]
  }
}